#!/bin/sh

PRG=$0
progname=`basename $0`
case "`uname -m`" in
    i[3-6]86  | ia32 | ia64)
        proc=i386
        ;;
    sparc*)
        proc=sparc
        ;;
    *)
        proc="`uname -m`"
        ;;
esac

# Resolve symlinks. See 4152645.
while [ -L "$PRG" ]; do
    ls=`/bin/ls -ld "$PRG"`
    link=`/usr/bin/expr "$ls" : '.*-> \(.*\)$'`
    if /usr/bin/expr "$link" : '/' > /dev/null; then
	PRG="$link"
    else
	PRG="`/usr/bin/dirname $PRG`/$link"
    fi
done

APPHOME=`dirname "$PRG"`/.. 

java -classpath $APPHOME/lib/karelj.jar karel.world.WorldEditor $@
